<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="DingTalkRobotAppender" type="log4net.Appender.DingTalkRobotAppender, log4net.Appender.DingTalkRobotAppender">
    <RobotSettings>
      <WebhookAddr>https://oapi.dingtalk.com/robot/send?access_token=59bdfd38e990c75d8daa950beace37ed697281e423e866cc90ca4080dc2a3f0d</WebhookAddr>
    </RobotSettings>

    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%d [%t] %-5p %c %m%n" />
    </layout>

    <filter type="log4net.Filter.LevelRangeFilter">
      <param name="LevelMin" value="ERROR" />
      <param name="LevelMax" value="FATAL" />
    </filter>
  </appender>
  <root>
    <level value="DEBUG"/>
    <appender-ref ref="DingTalkRobotAppender" />
  </root>
</log4net>